<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bundle.BundleController">
   <children>
      <HBox spacing="10.0">
         <children>
            <Button fx:id="btnAccept" text="%btn_accept" />
            <Button fx:id="btnReload" text="%btn_reload" />
            <Button fx:id="btnCancel" text="%btn_cancel">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</HBox>
